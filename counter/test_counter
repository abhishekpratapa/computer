    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 8;
    %cassign/vec4 v0x56121c293a60_0;
    %jmp T_1.1;
T_1.0 ;
    %deassign v0x56121c293a60_0, 0, 8;
T_1.1 ;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x56121c273610;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x56121c293d60_0, 0, 1;
